<ul>
  <% @users.each do |user| %>
    <li>
        <div class="delete-button">
            <%= button_to "Delete",
                        admin_user_path(user),
                        method: :delete %>
        </div>
        <div class="card">
            <%= user.created_at %>,
            <%= user.id %>,
            <%= user.open_id %>
        </div>
    </li>
  <% end %>
</ul>